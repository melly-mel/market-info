{
    "compilerOptions": {
        "outDir": "build",
        "jsx": "react-jsx",
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "allowJs": true,
        "esModuleInterop": true
    },
    "exclude": ["node_modules", "build/**/*"]
}